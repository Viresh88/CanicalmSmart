<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/getstarted"
    android:gravity="center_vertical"
    tools:context=".GetStarted">


  <ImageView
      android:id="@+id/imageView"
      android:layout_width="120dp"
      android:layout_height="18dp"
      android:layout_marginStart="26dp"
      android:layout_marginLeft="26dp"
      android:layout_marginTop="446dp"
      android:layout_marginBottom="20dp"
      android:background="@color/white"
      android:src="@drawable/numaxesimg"
      app:layout_constraintBottom_toTopOf="@+id/textView12"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/textView12"
      android:layout_width="263dp"
      android:layout_height="48dp"
      android:layout_marginStart="24dp"
      android:layout_marginBottom="40dp"
      android:layout_marginTop="477dp"
      android:fontFamily="@font/poppins"
      android:text="Canicalm Smart"
      android:lineHeight="48dp"
      android:textColor="#262727"
      android:textFontWeight="400"
      android:textSize="32sp"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView" />

  <!--  <ImageView-->
  <!--      android:id="@+id/btngetStarted"-->
  <!--      android:layout_width="wrap_content"-->
  <!--      android:layout_height="wrap_content"-->
  <!--      android:layout_marginTop="30dp"-->
  <!--      android:src="@drawable/buttongetstarted"/>-->

  <Button
      android:id="@+id/btngetStarted"
      android:layout_width="327dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="9dp"
      android:layout_marginLeft="9dp"
      android:layout_marginEnd="9dp"
      android:layout_marginRight="9dp"
      android:background="@drawable/rounded_button"
      android:text="Get Started"
      android:textAllCaps="true"
      android:textColor="@android:color/white"
      android:fontFamily="@font/poppins"
      android:textFontWeight="600"
      android:textSize="18sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView12" />


</androidx.constraintlayout.widget.ConstraintLayout>